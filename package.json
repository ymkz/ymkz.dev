{
  "name": "ymkz.co",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build && next export",
    "dev": "next dev",
    "fmt": "npm run fmt:prettier && npm run fmt:next",
    "fmt:next": "next lint --fix",
    "fmt:prettier": "prettier --write .",
    "lint": "npm run lint:prettier && npm run lint:next && npm run lint:tsc",
    "lint:next": "next lint",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.38",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.5.1",
    "prettier-plugin-css-order": "1.0.0",
    "prettier-plugin-pkg": "0.11.1",
    "typescript": "4.5.4"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
