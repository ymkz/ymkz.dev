{
  "name": "ymkz.co",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "git config --local core.hooksPath .githooks || echo 'CI'",
    "reset": "rm -rf node_modules pnpm-lock.yaml",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npm run lint:tsc && npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "fmt": "npm run fmt:prettier && npm run fmt:eslint",
    "fmt:prettier": "prettier --write .",
    "fmt:eslint": "eslint --fix ."
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,yml,yaml,json,html,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "solid-js": "1.0.7",
    "solid-styled-components": "0.27.1"
  },
  "devDependencies": {
    "@types/babel__core": "7.1.15",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "lint-staged": "11.1.1",
    "prettier": "2.3.2",
    "prettier-plugin-organize-imports": "2.3.3",
    "prettier-plugin-packagejson": "2.2.11",
    "typescript": "4.3.5",
    "vite": "2.4.4",
    "vite-plugin-solid": "2.0.1",
    "vite-tsconfig-paths": "3.3.13"
  },
  "engines": {
    "node": "16.x"
  },
  "volta": {
    "node": "16.6.1"
  }
}
