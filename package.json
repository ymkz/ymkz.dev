{
  "name": "ymkz.co",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "git config --local core.hooksPath .githooks || echo 'CI'",
    "reset": "rm -rf node_modules package-lock.json",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "codecheck": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint": "npm run typecheck && npm run codecheck",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,yml,json,html,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "solid-js": "1.0.4",
    "solid-styled-components": "0.27.1"
  },
  "devDependencies": {
    "@types/babel__core": "7.1.15",
    "lint-staged": "11.1.0",
    "prettier": "2.3.2",
    "prettier-plugin-organize-imports": "2.3.3",
    "prettier-plugin-packagejson": "2.2.11",
    "typescript": "4.3.5",
    "vite": "2.4.4",
    "vite-plugin-solid": "2.0.1",
    "vite-tsconfig-paths": "3.3.13"
  },
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "volta": {
    "node": "16.4.2",
    "npm": "7.19.1"
  }
}
